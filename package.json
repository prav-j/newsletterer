{
  "name": "newsletterer",
  "version": "0.0.1",
  "engines": {
    "node": ">= 14.15 <15"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.9",
    "@types/jest": "~26.0.15",
    "@types/morgan": "^1.9.2",
    "@types/node": "~14.14",
    "@typescript-eslint/eslint-plugin": "~4.7.0",
    "@typescript-eslint/parser": "~4.7.0",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "lint": "eslint . --ext .ts,.tsx",
    "serve": "nodemon --delay 1 build/src/main.js",
    "start": "yarn clean && yarn build && yarn watch",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn build:watch\" \"yarn serve\""
  },
  "author": "Praveen Janakarajan",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.4.3",
    "eslint": "~7.13.0",
    "eslint-config-prettier": "~6.15.0",
    "eslint-plugin-jest": "~24.1.3",
    "express": "^4.17.1",
    "jest": "~26.6.3",
    "morgan": "^1.7.0",
    "pg": "6.4.1",
    "prettier": "~2.1.2",
    "rimraf": "~3.0.2",
    "sequelize": "^4.3.2",
    "sequelize-typescript": "^0.3.5",
    "ts-jest": "~26.4.4",
    "tslib": "^2.0.3",
    "tsutils": "~3.17.0",
    "typescript": "~4.0.5",
    "uuid": "^3.1.0"
  }
}
