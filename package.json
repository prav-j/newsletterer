{
  "name": "newsletterer",
  "version": "0.0.1",
  "engines": {
    "node": ">= 14.15 <15"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/morgan": "^1.9.2",
    "@types/node": "~14.14",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "~4.7.0",
    "@typescript-eslint/parser": "~4.7.0",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "lint": "eslint . --ext .ts,.tsx",
    "prebuild": "yarn clean",
    "prewatch": "yarn build",
    "serve": "nodemon --delay 1 build/src/main.js",
    "start": "yarn watch",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn build:watch\" \"yarn serve\""
  },
  "author": "Praveen Janakarajan",
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.4.3",
    "eslint": "~7.13.0",
    "eslint-config-prettier": "~6.15.0",
    "eslint-plugin-jest": "~24.1.3",
    "express": "^4.17.1",
    "jest": "~26.6.3",
    "morgan": "^1.7.0",
    "pg": "^8.5.1",
    "prettier": "~2.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "~3.0.2",
    "sequelize": "^5.1.0",
    "sequelize-typescript": "^1.1.0",
    "sqlite3": "^5.0.0",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "tsutils": "~3.17.0",
    "typescript": "~4.0.5",
    "uuid": "^8.3.1",
    "validator": "^13.1.17"
  }
}
